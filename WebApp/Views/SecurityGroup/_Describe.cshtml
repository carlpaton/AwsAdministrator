@model List<WebApp.Models.DescribeSecurityGroupModel>

@for (int i = 0; i < Model.Count(); i++)
{
    <dl class="row">
        <dt class="col-sm-2">
            <label asp-for="@Model[i].SecurityGroupId"></label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model[i].SecurityGroupId)
        </dd>
    </dl>
}