@model List<WebApp.Models.DescribeRouteTableModel>

<h4>Route Tables</h4>
@for (int i = 0; i < Model.Count(); i++)
{
    <dl class="row">
        <dt class="col-sm-2">
            <label asp-for="@Model[i].RouteTableId"></label>
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model[i].RouteTableId)
        </dd>
    </dl>
}